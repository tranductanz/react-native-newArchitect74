{
    "env": {
        "production": {
            "plugins": [
                "transform-remove-console"
            ]
        }
    },
    "presets": [
        "module:@react-native/babel-preset"
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        [
            "module-resolver",
            {
                "root": [
                    "./"
                ],
                "alias": {
                    "@common": "./app/common/index",
                    "@components": "./app/components/index",
                    "@assets": "./app/assets/index",
                    "@styles": "./app/styles/index",
                    "@constants": "./app/constants/index",
                    "@globalStore": "./app/global/index"
                }
            }
        ]
    ]
}